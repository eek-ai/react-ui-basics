    "build": "./build.sh",
    "postinstall": "",
    "clean": "rm -rf build && mkdir build",
    "dist": "npm run clean && npm run build && npm run build-storybook && cd build && npm publish",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -c .storybook -o docs"
